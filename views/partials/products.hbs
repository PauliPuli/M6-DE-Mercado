{{#each products}}
<div class="col-4 mb-4">
    <div class="card">
        <div style="width: 12rem; display: inline-block; margin: 0 auto">
            <img src="imgs/{{this}}.png" class="card-img-top" alt="{{this}}">
        </div>
        <div class="card-body">
            <h5 class="card-title">{{this}}</h5>
        </div>
    </div>
</div>
{{/each}}